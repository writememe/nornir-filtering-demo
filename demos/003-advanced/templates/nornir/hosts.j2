---
# Autogenerated nornir file
{% for dev in inventory %}
{{ dev.name }}:
    hostname: {{ dev.name }}
    groups:
        - {{ dev.operating_system }}
        - {{ dev.environment }}
        - {{ dev.site_code }}
    data:
        mgmt_ip: {{ dev.mgmt_ip }}
        vendor: {{ dev.vendor|lower }}
        device_type: {{ dev.device_type }}
        os_version: {{ dev.os_version }}
        site_code: {{ dev.site_code }}
        
{% endfor %}